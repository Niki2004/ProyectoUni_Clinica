@{
    ViewBag.Title = "Confirmación de restablecimiento de contraseña";
}

<main aria-labelledby="title">
    <hgroup class="title">
        <h1 id="title">@ViewBag.Title.</h1>
    </hgroup>
    <div>
        <p>
            Tu contraseña ha sido restablecida. Por favor @Html.ActionLink("Haga clic aquí para Seguir", "PerfilSEC", "Perfil", routeValues: null, htmlAttributes: new { id = "loginLink" })
        </p>
    </div>
</main>
